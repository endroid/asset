{
    "name": "endroid/asset",
    "description": "Endroid Asset",
    "keywords": [
        "endroid",
        "asset"
    ],
    "homepage": "https://github.com/endroid/asset",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Jeroen van den Enden",
            "email": "info@endroid.nl"
        }
    ],
    "require": {
        "php": "^8.4",
        "endroid/installer": "^1.5",
        "symfony/cache-contracts": "^3.0",
        "symfony/http-client": "^6.4||^7.4||^8.0",
        "symfony/http-kernel": "^6.4||^7.4||^8.0",
        "symfony/options-resolver": "^6.4||^7.4||^8.0",
        "twig/twig": "^3.0"
    },
    "require-dev": {
        "endroid/quality": "dev-main"
    },
    "suggest": {
        "roave/security-advisories": "Avoids installation of package versions with vulnerabilities"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "carthage-software/mago": true,
            "endroid/installer": true
        }
    },
    "autoload": {
        "psr-4": {
            "Endroid\\Asset\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Endroid\\Asset\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-main": "2.x-dev"
        }
    }
}
